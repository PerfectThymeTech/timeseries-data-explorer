{
	"name": "IptvUpload",
	"properties": {
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "IptvMainPipeline",
					"type": "PipelineReference"
				},
				"parameters": {
					"uploadSourceFolderPath": "@triggerBody().folderPath",
					"uploadSourceFileName": "@triggerBody().fileName"
